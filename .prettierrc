{
  "plugins": ["prettier-plugin-tailwindcss"],
  "tailwindConfig": "./tailwind.config.js",
  "singleQuote": true,
  "trailingComma": "es5",
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 80,
  "quoteProps": "consistent",
  "vueIndentScriptAndStyle": true,
  "useTabs": true,
  "tabWidth": 2,
  "semi": true
}
